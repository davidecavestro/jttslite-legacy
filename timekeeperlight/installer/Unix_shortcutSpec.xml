<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<shortcuts>
   <skipIfNotSupported/>
   <programGroup defaultName="@{external.name}"
				 location="applications"/>
   <shortcut
	  name="@{external.name}"
	  target="$JAVA_HOME/bin/java"
	  commandLine="-jar $INSTALL_PATH/timekeeperlight.jar"
	  description="@{external.name}"
	  iconFile="@{basedir}/../deploy/icons/jtts48x48.png"
	  workingDirectory="$INSTALL_PATH"
	  initialState="normal"
	  programGroup="no"
	  desktop="yes"
	  applications="no"
	  startMenu="no"
	  startup="no"
	  type="Application"
	  terminal="true"
	  encoding="UTF-8">
	  <createForPack name="Required"/>
   </shortcut>
</shortcuts>
